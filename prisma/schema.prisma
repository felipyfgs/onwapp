generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Session {
  id            String   @id @default(uuid())
  name          String   @unique
  status        String   @default("disconnected")
  qrCode        String?
  phoneNumber   String?
  creds         Json?
  webhookUrl    String?
  webhookEvents String[] @default([])
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt

  @@map("sessions")
}
