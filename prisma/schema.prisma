generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Session {
  id        String   @id @default(cuid())
  name      String   @unique
  qrcode    String?
  status    String   @default("disconnected")
  phone     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
